<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Menu"
        tools:targetApi="31">

        <activity android:name=".CalculatorActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ChronometerActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".CreativeActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".SistemaSolarActivity"
            android:parentActivityName=".CreativeActivity" />
        <activity android:name=".MercurioActivity"
            android:parentActivityName=".SistemaSolarActivity" />
        <activity android:name=".VenusActivity"
            android:parentActivityName=".SistemaSolarActivity" />
        <activity android:name=".TierraActivity"
            android:parentActivityName=".SistemaSolarActivity" />
        <activity android:name=".MarteActivity"
            android:parentActivityName=".SistemaSolarActivity" />
        <activity android:name=".JupiterActivity"
            android:parentActivityName=".SistemaSolarActivity" />
        <activity android:name=".SaturnoActivity"
            android:parentActivityName=".SistemaSolarActivity" />
        <activity android:name=".UranoActivity"
            android:parentActivityName=".SistemaSolarActivity" />
        <activity android:name=".NeptunoActivity"
            android:parentActivityName=".SistemaSolarActivity" />


        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
